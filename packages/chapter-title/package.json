{
  "name": "@ssgw/chapter-title",
  "version": "0.2.0",
  "description": "Animated text used in chapter bookends",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist && cp src/*.scss $_",
    "build": "../../node_modules/.bin/babel --root-mode upward src/*.js -d dist --source-maps --ignore \"src/**/*.stories.js\"",
    "webpack": "../../node_modules/.bin/webpack ./src/index.js --o ./dist/index.js --config ../../config/webpack.config.js",
    "clean": "rm -rf dist node_modules",
    "publish": "npm publish || echo 'could not publish, skipping'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Tim Hettler",
  "license": "UNLICENSED",
  "dependencies": {
    "@ssgw/utils": "^0.1.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "node-sass": "4.10.x",
    "react": "16.6.x",
    "react-dom": "16.6.x"
  }
}
